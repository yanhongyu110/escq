.ck-product-list ul li:last-child{margin-right:0;}
.item-panel {width: 260px;border: 1px solid #e6e6e6;background-color: #ffffff;font-size: 12px; position: relative;}

.item-pic { width: 260px;height: 156px;overflow: hidden;position: relative;z-index: 1;margin: 0px 0 10px 0;}
.item-pic .ico-item-dj,
.item-pic .ico-item-yj,
.item-pic .ico-item-top {position: absolute;top: 0;left: 0;}
.item-pic .ico-item-st {position: absolute;top: 0;right: 0;}
.item-pic img {transition: transform .25s ease 0s;}
.item-pic img:hover {transform: scale(1.05);}
.item-title {display: block;margin: 10px 15px 8px;color: #666;font-size: 14px;overflow: hidden;}
.item-title a{ color: #333; text-decoration: none;}
.item-title a:hover {color: #FB5A5A; text-decoration: none;}
.item-info {margin: -2px 15px 13px;height: 34px;color: #999;overflow: hidden;}
.item-addition {position: relative;padding:0px 15px;height: 38px;overflow: hidden;}
.item-addition:after {visibility: hidden;display: block;font-size: 0;content: " ";clear: both;height: 0;}
.item-addition .price {float: left;max-width: 100px;overflow: hidden;height: 22px;line-height: 16px;color: #bbb;text-overflow: ellipsis;white-space: nowrap;vertical-align: baseline;}
.item-addition .price em {margin-right: 3px;font-size: 16px;}
.item-addition .price .hot-price,
.item-addition .price .off-price {font-size: 18px;color: #f60;}
.item-addition .price .off-price {color: #ccc;text-decoration: line-through;}
.item-addition .free {font-size: 18px;color: #FB5A5A; font-weight: 600;font-family: arial;}
.item-addition .number {float: right;width: 84px; white-space: nowrap; text-overflow: ellipsis;overflow: hidden;text-align: right;line-height:35px;color: #bbb;}
.item-addition .number em {color: #999;    font-style: normal;font-family: "Microsoft YaHei";}
.item-panel .satisfaction {float: left;width: 90px;line-height: 35px;color: #bbb;}
.l_more{margin:50px auto;width:160px;text-align:center;line-height: 30px;height:30px;color:#999;font-size:16px;border-radius:5px;cursor: pointer;}
.l_more:hover{color:#fb5a5a;}
.mes_tss{display:none;color:#999;clear:both;width: 100%;text-align: center;margin:50px auto;font-size:16px;}
.fw_xg{width:240px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;padding-left:15px;    line-height: 30px;}
 .appendcontent{clear:both;}
 .responseContent, .appendcontent1, .responseAppendContent{margin-top:0;}
 .appendcontent p{line-height:28px;}
  .select_pj{margin-bottom:20px;padding:10px;background:#eee;}
  .service_content_pj_list_title span b{color:#666}
 .select_pj input{width:17px;height:17px;vertical-align: bottom;font-size:16px;}
.note_message h3 {text-align:center;color:#999;}
.ck-product-list .item-panel .server_tx{margin-bottom:20px}
.ck-product-list .item-panel .server_tx ul{padding: 15px 15px 0 15px; overflow: hidden; width: 100%;}
.ck-product-list .item-panel .server_tx ul li{ float: left;width: 20%;}
.ck-product-list .item-panel .server_tx ul li img{width: 50px; height:50px; border-radius: 50%;}
.ck-product-list .item-panel .server_tx ul li p span{float: left;}
.ck-product-list .item-panel .server_tx ul li p{clear: both;width: 170px;overflow: hidden; white-space: nowrap; text-overflow: ellipsis;}
.ck-product-list .item-panel .server_tx ul li p span:last-child{float: right;border: 1px solid #fb5a5a;padding:5px 10px;color: #fb5a5a;margin-right: 10px;}
.ck-product-list .item-panel .server_tx ul li:last-child{width: 65%;}
.ck-product-list .item-panel .server_tx ul li p span:last-child:hover{border:1px solid #fb5a5a;color: #fb5a5a; cursor: pointer;}


.map_dw_left{float:left;}
.mkq-list-3 { z-index: 10; background: #fff; position: relative;clear:both;height:350px}
.mkq-list-3 .map_dw {    position: absolute;width: 600px; height: 248px; top: 70px; }
.mkq-list-3 .local { width: 100px; height: 100px;background: url(../images/dw.png) no-repeat; position: absolute; opacity: 0; left: 250px; z-index: 7;top:30px}
.mkq-list-3 .mkq { width: 660px; height: 200px; background: url(../images/mk-list-image.png) no-repeat;  position: absolute; opacity: 0; top:25px; left:70px; z-index: 6;}
.mkq-list-3 .circle-image { width: 577px; height: 212px;position: absolute; opacity: 0; top: 36px; z-index: 5;}
.mkq-list-3 .circle-line { width: 100%; height: 100%; border: 2px solid #bbb; border-radius: 100%; position: absolute;left: 4px; top: -8px; z-index: 5; transform: scale(0); -webkit-transform: scale(0); -moz-transform: scale(0); }
.mkq-list-3 .map_dw_text { float:right;    padding-top: 100px;}
.mkq-list-3 .map_dw_text p.large { font-size: 40px; line-height: 40px; letter-spacing: 6px;}
.mkq-list-3 .map_dw_text p.small { font-size: 20px; margin-top: 18px;padding-bottom: 50px; line-height: 20px; letter-spacing: 0.5px;}
.mkq-list-3 .local.animation { opacity: 1; -webkit-animation: local 3s infinite forwards linear; animation: local 3s infinite forwards linear; }
.mkq-list-3 .mkq.animation { opacity: 1; -webkit-animation: mkq 2.3s 1 forwards ease-out; animation: mkq 2.3s 1 forwards ease-out; }
.mkq-list-3 .circle-1.animation { -webkit-animation: circle 5s 0s infinite forwards ease; animation: circle 5s 0s infinite forwards ease; }
.mkq-list-3 .circle-image.circle-2.animation { -webkit-animation: circle 5s 2.5s infinite forwards ease; animation: circle 5s 2.5s infinite forwards ease; }
.mkq-list-3 .circle-line.circle-2.animation { -webkit-animation: circle 5s 1s infinite forwards ease; animation: circle 5s 1s infinite forwards ease; }
.mkq-list-3 .circle-line.circle-3.animation { -webkit-animation: circle 5s 2s infinite forwards ease; animation: circle 5s 2s infinite forwards ease; }
.mkq-list-3 .circle-line.circle-4.animation { -webkit-animation: circle 5s 3s infinite forwards ease; animation: circle 5s 3s infinite forwards ease; }
.mkq-list-3 .circle-line.circle-5.animation { -webkit-animation: circle 5s 4s infinite forwards ease; animation: circle 5s 4s infinite forwards ease; }
@-webkit-keyframes local{
	0% { top: -15px;}
	50% { top: 0;}
	100% { top: -15px;}
}
@keyframes local{
	0% { top: -15px;}
	50% { top: 0;}
	100% { top: -15px;}
}
@-webkit-keyframes mkq{
	0% { -webkit-transform:scale(0);-moz-transform:scale(0);-o-transform:scale(0); transform:scale(0)}
	100% { opacity: 1; filter:alpha(opacity=0); -webkit-transform:scale(1); -moz-transform:scale(1); -o-transform:scale(1); transform:scale(1);}
}
@keyframes mkq{
	0% { -webkit-transform:scale(0);-moz-transform:scale(0);-o-transform:scale(0); transform:scale(0)}
	100% { opacity: 1; filter:alpha(opacity=0); -webkit-transform:scale(1); -moz-transform:scale(1); -o-transform:scale(1); transform:scale(1);}
}

@-webkit-keyframes circle{
	0%{-webkit-transform:scale(0.1,0.1); opacity: 1; -moz-transform: scale(0.1,0.1); opacity: 1; transform: scale(0.1,0.1); opacity:1;}
	100%{-webkit-transform:scale(1,1); opacity: 0; -moz-transform: scale(1,1); opacity: 0; transform: scale(1,1); opacity: 0;}
}
@-moz-keyframes circle{
	0%{-webkit-transform:scale(0.1,0.1); opacity: 1; -moz-transform: scale(0.1,0.1); opacity: 1; transform: scale(0.1,0.1); opacity:1;}
	100%{-webkit-transform:scale(1,1); opacity: 0; -moz-transform: scale(1,1); opacity: 0; transform: scale(1,1); opacity: 0;}
}
@keyframes circle{
	0%{-webkit-transform:scale(0.1,0.1); opacity: 1; -moz-transform: scale(0.1,0.1); opacity: 1; transform: scale(0.1,0.1); opacity:1;}
	100%{-webkit-transform:scale(1,1); opacity: 0; -moz-transform: scale(1,1); opacity: 0; transform: scale(1,1); opacity: 0;}
}

