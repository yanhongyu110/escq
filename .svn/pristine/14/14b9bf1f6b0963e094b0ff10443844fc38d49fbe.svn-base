	                for (var i = 0; i < typeList.length; i++) {
	                    var commonType = typeList[i].text;
	                    var id = typeList[i].id;
	                    $("#mmyull").append("<li style='color:#fff;text-align:center;height:50px;line-height:50px;border-bottom: 1px solid rgba(255,255,255,.1);' val=" + id + ">" + commonType + "</li>");
	                }
                }
            }
        })
    }
