.service_content{
	margin-bottom: 61px;
	margin-top:20px;

}
.service_content_pj{
	margin-top:0px !important;
	/* margin-bottom: 15px; */
}
.service_content_pj ul{
	padding: 15px;
    margin:0;
    background: #fff;
    border-bottom:1px solid #eee;
}
.service_content_pj ul .news_title{
	width: 80%;
    display: inline-block;
	font-size: 14px;
    margin-bottom: 10px;
}
.service_content_pj ul .news_time{
	width: 20%;
    display: inline-block;
    text-align: right;
}
.service_content_pj ul .news_content p{
	margin-bottom: 10px;
	font-size: 14px;
}
.service_content_pj ul .news_content span{
	color:#a6a6a6;
	font-size: 12px;
}
.loading{
	
	text-align: center;
	line-height: 50px;
	font-size: 16px;
}
.my_date_bt .title{
	position: fixed;
    top: 0;
    z-index: 100;
    width: 100%;
}