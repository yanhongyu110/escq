.service_content_pj_list{ border-bottom: 1px solid #eee; padding: 0; clear: both; padding: 24px 0 16px 0px; overflow: hidden;}
.service_content_pj_list_img{ float: left; width: 100px; }
.service_content_pj_list_img h3{ font-size: 14px; font-weight: normal; text-align: center; width: 50px; overflow: hidden; white-space: nowrap; text-overflow: ellipsis; height:30px;line-height:30px;}
.service_content_pj_list_img img{  width:50px;height:50px; border-radius: 50%;}
.service_content_pj_list_title{ float: left; }
.service_content_pj_list_title h3{ font-size: 14px; padding-bottom:5px;}
.service_content_pj_list_zan{  color: #999;background: url(../images/zan.png) no-repeat; padding-left: 20px;}
.service_content_pj_list_title span{ color: #999; margin-right: 20px;}
.service_content_pj_list_title p{ width:500px; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; line-height: 28px; }
.fu_pj{ float: right;}
.fu_pj p{ text-align: center; padding-top: 10px; font-size: 16px; color: #999;}
.service_content_pj_list:last-child{ border: 0;}
.service_content_pj_list .select_pj{font-size: 14px;background: #eee;padding: 10px;}
.service_content_pj_list .select_pj input{width: 17px;height: 17px;vertical-align: bottom;font-size: 16px;}

.admin_pl_table{ clear: both; margin-top: 20px; margin-bottom: 20px;}
.admin_pl_table table{ width: 100%; border: 1px solid #DAF3FF; border-bottom: 0; border-right: 0;    background: #fff;}
.admin_pl_table table td{ text-align: center; padding: 10px; border-bottom: 1px solid #DAF3FF; border-right: 1px solid #DAF3FF;}
.admin_pl_table table tr:first-child{ font-weight: 600;}
.admin_pl_table table tr td:first-child{ font-weight: 600;}
 ._b_tt_tt{width:100%;overflow: hidden;margin-top: 40px;}
._b_tt_tt .conta {width: 100%; margin:20px 0px;overflow: hidden;font-size:14px;}

._b_tt_tt .conta .le { width: 15%; float: left;overflow: hidden;line-height:22px;}

._b_tt_tt .conta .ri { width: 55%;float: left;overflow: hidden;line-height:22px; }

 ._b_tt_tt .conta .tt{float: left;font-size: 12px; color: #999;width: 25%; margin-left:5%;line-height:22px;}
