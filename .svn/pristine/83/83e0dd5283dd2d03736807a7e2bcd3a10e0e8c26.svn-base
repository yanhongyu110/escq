
html,body{
	width: 100%;
	height: 100%;
	margin: 0;
	padding: 0;
	overflow: hidden;
}


#allmap {
	height: 90%;
	width: 100%;
	float: left;
}

.searchInput{
	width: 100%;
	height: 50px;
	float: left;
	border-bottom: 1px solid #cad3e0;
	font-family:"풕턼환쥌";
	font-style: 12px;
	background: #fafafa;
}

.searchPanel{
	width: 100%;
	height: 40px;
}

.searchPanel .txtContent{
	width: 80%;
	height: 40px;
	margin: 5px 0;
	border: 1px solid #dcdcdc;
	border-right: none;
	padding: 0 5px;
	font-family: "풕턼환쥌";
	color: #999;
	float: left;
}

.searchPanel a.searchBtn{
	width: 19%;
	height: 38px;
	margin: 5px 0;
	border:1px solid #dcdcdc;
	background: #f5f5f5;
	font-family: "풕턼환쥌";
	color: #666;
	cursor: pointer;
	display: block;
	text-align: center;
	line-height: 40px;
	text-decoration: none;
	float: left;
	font-size: 14px;
}
.resultPanel{
	width: 100%;
	height: 100%;
	background: #fafafa;
	display: none;
	position: absolute;
	z-index: 99;
	top:50px;
	left: 0;
}
.resultPanel ul.resultMap{
	width: 100%;
	margin-top: 0; 
	list-style: none;
	padding: 0;
	display: block;
	overflow-x:hidden;
	/*overflow-y:auto; */
}

/* 촳쐜햰픽 */
.resultPanel ul.resultMap li{
	height: 80px;
	display: block;
	border-bottom: 1px solid #cad3e0;
	font-size: 13px;
	padding: 15px;
	cursor: pointer;
}

.resultPanel ul.resultMap li:hover{
	background: #eee;
}
/* 훱췲 */
.resultPanel ul.resultMap li.pageList{
	border: none;
	height: 40px;
	cursor: default;
}
.resultPanel ul.resultMap li.pageList:hover{
	background: #fafafa;
}
.resultPanel ul.resultMap li.pageList a{
	display: block;
	margin-right: 10px;
	float: left;
	text-decoration: none;
	font-size: 16px;
}

.addrTotal{
	width: 100%;
	height: 45px;
	line-height: 45px;
	display: block;
	
	font-size: 14px;
}

.addrTotal b{
	color: #17b5b5;
}
.addrTotal #tot{
	float: left;
	padding-left: 15px;
}
.addrTotal #cal{
	float: right;
	margin-right: 30px;
	color: #f00;
	text-decoration: none;
}
.failResult{
	font-size: 14px;
	padding-left: 15px;
}

.infoWindow_main{
	width: 290px;
}
.infoWindow_left{
	width: 200px;
	list-style: none;
	float: left;
	padding: 0;
}
.infoWindow_left li{

}

.infoWindow_right{
	width: 100px;
	height: 100px;
	border:1px solid #eee;
	float: right;
}
