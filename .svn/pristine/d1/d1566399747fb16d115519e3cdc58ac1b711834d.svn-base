		<SCRIPT type=text/javascript>
			var selectListShow = 0;
			$(function() {
				$("#type,.searchselectbtn").click(function() {
					if(selectListShow) {
						$("#selectTypeList").slideUp("fast");
						selectListShow = 0;
					} else {
						$("#selectTypeList").slideDown("fast");
						selectListShow = 1;
					}
					return false;
				});
				$("body").click(function() {
					if(selectListShow) {
						$("#selectTypeList").slideUp("fast");
						selectListShow = 0;
					}
				});
				$(".searchselect li").click(function() {
					$("#type").text($(this).text());
					$("#type").attr("typename", $(this).attr("typename"));
					$(this).parent().hide();
				});

			});
		</SCRIPT>
		<style>
			.swiper-container {
				height: 7rem;
				margin-top: 20px;
			}
			
			.swiper-slide {
